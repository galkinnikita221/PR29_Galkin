<Window x:Class="PR29_Galkin.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PR29_Galkin"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid x:Name="AllButtons" Visibility="Visible">
            <Button Click="Clubs" Height="25" Content="Клубы" VerticalAlignment="Top" Margin="10" HorizontalAlignment="Left" Width="100" Background="#FF46FD00" BorderBrush="#FF46FD00" Foreground="White"/>
            <Button Click="Users" Height="25" Content="Пользователи" VerticalAlignment="Top" Margin="120,10,0,0" HorizontalAlignment="Left" Width="100" Background="#FF46FD00" BorderBrush="#FF46FD00" Foreground="White"/>
        </Grid>
        <Frame x:Name="frame" NavigationUIVisibility="Hidden" Margin="0,40,0,0"/>
    </Grid>
</Window>
